#include<stdio.h>

int main() {

    int array[5][5],B[5][5];

    printf("enter elements for A matrix\n");
    for(int i=0; i<3; i++){
        for(int j=0; j<4; j++){
            printf("A[%d][%d] = ",i,j);
            scanf("%d",&array[i][j]);
        }
    }

    for(int i=0; i<3; i++){
        for(int j=0; j<4; j++){
            printf("%d ",array[i][j]);
        }
        printf("\n");
    }


    printf("enter elements for B matrix\n");
    for(int i=0; i<3; i++){
        for(int j=0; j<4; j++){
            printf("A[%d][%d] = ",i,j);
            scanf("%d",&array[i][j]);
        }
    }

    for(int i=0; i<3; i++){
        for(int j=0; j<4; j++){
            printf("%d ",array[i][j]);
        }
        printf("\n");
    }

    
    return 0;
}
