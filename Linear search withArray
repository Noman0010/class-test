#include<stdio.h>

int main(){

    int arr[]={10,2,15,20,35,46,85};
    int x;
    scanf("%d",&x);
    int count = -1;

    for(int i=0; i<7; i++){
        if(x==arr[i]){
            count = i+1;
            break;
        }
    }

    if(count==-1){
        printf("elements is absent");
    }
    else{
        printf("element is found at %d",count);
    }


    return 0;
}
